
class Solution {
    
    public String reverse(String S){
        //code here
        Stack<Character> st = new Stack<>();
        for(int i =0 ; i < S.length();i++)
        {
            char ch = S.charAt(i);
            st.push(ch);
        }
        // System.out.println(st);
        String s = "";
        while(st.size()>0)
        {
            s += st.pop()+"";
        }
        return s;
    }

}
