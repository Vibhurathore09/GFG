String removeDups(String S) {
        // code here
        //char ch[] = S.toCharArray();
        HashSet<Character> s = new HashSet<Character>();
        String s1="";
        for(int i=0;i<S.length();i++)
        {
            char c =S.charAt(i);
            if(!s.contains(c))
            {
                s1+=c;
            }
            s.add(c);
        }
        
        return s1;
    }
