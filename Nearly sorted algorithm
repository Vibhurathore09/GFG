import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	 //code
	 Scanner sc = new Scanner(System.in);
	 int n = sc.nextInt();
	 while(n-->0)
	 {
	     int a = sc.nextInt() , b = sc.nextInt();
	     int arr[] = new int[a];
	     for(int i=0;i<a;i++)
	     {
	         arr[i] = sc.nextInt();
	     }
	     Arrays.sort(arr);
	     for(int i:arr)
	     {
	         System.out.print(i+" ");
	     }
	     System.out.println();
	 }
	 
	 }
}
