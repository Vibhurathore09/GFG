import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		//code
		int n ;
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		while(n-->0)
		{
		    String s = sc.next();
		    String p = sc.next();
		    
		    
		    while(s.contains(p+p))
		    {
		        s=s.replaceAll(p+p,p);
		    }
		    s=s.replaceAll(p,"X");
		    System.out.println(s);
		    
		}
	}
}
